prob.test <- rep(NA, 100)
prob.brms <- rep(NA, 100)
test <- data.frame(xval = xval,
prob.test = prob.test,
prob.brms = prob.brms)
for(i in 1:100) {
xval <- test[i, 1]
test[i, 2] <- dhlnorm(x = xval, meanlog = muval, sdlog = sdval, pval = 0.2)
test[i, 3] <- brms::dhurdle_lognormal(x = xval, mu = muval, sigma = sdval, hu = 0.2, log = FALSE)
}
