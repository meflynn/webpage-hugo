?brms::dhurdle_lognormal
prob.brms <- brms::dhurdle_lognormal(3, mu = muval, sigma = sdval, hu = 0.2, log = FALSE)
dhlnorm <- function(x, meanlog, sdlog, pval) {
if (x > 0) {
value <- dlnorm(x, meanlog = meanlog, sdlog = sdlog, log = TRUE) + log(1-pval)
return(exp(value))
} else {
value <- exp(pval)
return(value)
}
}
prob <- dhlnorm(3, meanlog = muval, sdlog = sdval, pval = 0.2)
print(prob)
xval <- seq(0, 20, length = 100)
prob <- rep(NA, 100)
test <- data.frame(xval = xval, prob = prob)
for(i in 1:100) {
xval <- test[i, 1]
test[i, 2] <- brms::dhurdle_lognormal(x = xval, mu = muval, sigma = sdval, hu = 0.2, log = FALSE)
}
View(test)
