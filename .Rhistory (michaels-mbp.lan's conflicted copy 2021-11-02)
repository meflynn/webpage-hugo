dhlnorm <- function(x, meanlog, sdlog, pval) {
if (x > 0) {
value <- dlnorm(x, meanlog = meanlog, sdlog = sdlog, log = TRUE) + log(1-pval)
return(exp(value))
} else {
value <- pval
return(value)
}
}
prob <- dhlnorm(3, meanlog = muval, sdlog = sdval, pval = 0.2)
print(prob)
